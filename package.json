{
  "name": "swiss-startup-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Swiss Startup Ecosystem Dashboard - Comprehensive analysis of Swiss companies and investment deals for SwissHacks 2025",
  "author": "SwissHacks 2025 Team",
  "license": "MIT",
  "homepage": ".",
  "keywords": [
    "swiss-startups",
    "investment-analysis",
    "startup-ecosystem",
    "data-visualization",
    "react-dashboard",
    "swisshacks-2025",
    "venture-capital",
    "swiss-companies",
    "funding-data",
    "d3-visualization",
    "tailwind-css"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "d3": "^7.9.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "gh-pages": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.24",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && npx serve -s build",
    "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "npx prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@10.14.0-0+sha512.2cd47a0cbf5f1d1de7693a88307a0ede5be94e0d3b34853d800ee775efbea0650cb562b77605ec80bc8d925f5cd27c4dfe8bb04d3a0b76090784c664450d32d6",
  "jest": {
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
